{
  "partition_size": "64",
  "bowtie2_path": "/path/to/required/apps/bowtie2-2.3.4.1-linux-x86_64",
  "bowtie2_index_path": "/path/to/bowtie2/index",
  "bowtie2_index_name": "bowtie2-index-name",
  "bowtie2_threads": "6",
  "annotations": {
    "bucket": "bucket-name",
    "path": "path/to/ensembl/annotations/annotations_ensembl.txt"
  },
  "samples":[
    {
    	"id": "sample-id",
    	"sample_format": "tab5",
        "sample_type": "paired",
        "streaming_app_name": "flint",
        "stream_dir": "s3a://bucket-name/path/to/sample/data/stream/",
        "batch_duration": "0.25",
        "output_dir": "/path/to/flint/output",
        "number_of_shards": "2"
    }
  ]
}
